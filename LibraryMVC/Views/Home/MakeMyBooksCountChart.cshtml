@model IEnumerable<string>

@{
    ViewBag.Title = "Make My Books Chart";
}

@{
    var myChart = new Chart(width: 550, height: 400, theme: ChartTheme.Blue)
        .AddTitle("My Books")
        .AddSeries(chartType: "Column",
            name: "Books",
            xValue: new[] { "Borrowed Books" },
            yValues: Model)
         .Write();
}
