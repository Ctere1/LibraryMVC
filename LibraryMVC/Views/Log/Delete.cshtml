@model LibraryMVC.Models.log

@{
    ViewBag.Title = "Log Delete";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <h3>Are you sure you want to delete this?</h3>
    <div>
        <h4>Delete Log</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.user_email)
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.user_email)
            </dd>
    
            <dt>
                @Html.DisplayNameFor(model => model.time)
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.time)
            </dd>
    
            <dt>
                @Html.DisplayNameFor(model => model.message)
            </dt>
    
            <dd style="background-color:#bebebe">
                @Html.DisplayFor(model => model.message)
            </dd>
    
        </dl>
    
        @using (Html.BeginForm()) {
            @Html.AntiForgeryToken()
    
            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-danger" /> |
                @Html.ActionLink("Back to List", "AdminLog")
            </div>
        }
    </div>
</body>
</html>
